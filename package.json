{
  "name": "EasyAccess",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "^10.16.3",
    "yarn": "^1.17.3"
  },
  "scripts": {
    "android": "ENVFILE=.env.development react-native run-android --variant developmentDebug",
