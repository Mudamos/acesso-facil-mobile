{
  "name": "EasyAccess",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "^10.16.3",
    "yarn": "^1.17.3"
  },
  "scripts": {
    "android": "ENVFILE=.env.development android/gradlew -p android installDevelopmentDebug && adb reverse tcp:8081 tcp:8081 && adb shell am start -n org.itsrio.sefaz.development.easyaccess/br.gov.rj.fazenda.acessofacil.MainActivity",
    "android-beta": "ENVFILE=.env.staging android/gradlew -p android installStagingDebug && adb reverse tcp:8081 tcp:8081 && adb shell am start -n org.itsrio.sefaz.staging.easyaccess/br.gov.rj.fazenda.acessofacil.MainActivity",
    "build-android-staging": "ENVFILE=.env.staging android/gradlew -p android assembleStagingRelease",
    "build-android-production": "ENVFILE=.env.production android/gradlew -p android bundleProductionRelease",
    "apks-android-production": "bundletool build-apks --bundle=android/app/build/outputs/bundle/productionRelease/app.aab --output=android/app/build/app.apks --ks=android/keystores/easyaccess-upload-production.keystore --ks-key-alias=easyaccess-upload-production",
    "install-android-apks-production": "bundletool install-apks --apks=android/app/build/app.apks",
    "ios": "ENVFILE=.env.development react-native run-ios --scheme EasyAccess",
    "lint": "eslint .",
    "log-android": "react-native log-android",
    "log-ios": "react-native-log-ios EasyAccess",
    "prettier": "prettier --write index.js src/*.js src/**/*.js",
    "start": "react-native start",
    "sort": "import-sort --write index.js src/*.js src/**/*.js",
    "postinstall": "jetify",
    "test": "jest",
    "xcode": "xed ios"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.6.1",
    "@react-native-community/masked-view": "^0.1.1",
    "@react-navigation/core": "^5.0.0-alpha.10",
    "@react-navigation/native": "^5.0.0-alpha.8",
    "@react-navigation/stack": "^5.0.0-alpha.17",
    "buffer": "^5.4.2",
    "farfetch": "^0.8.0",
    "humps": "^2.0.1",
    "ramda": "^0.26.1",
    "react": "16.8.6",
    "react-native": "0.60.5",
    "react-native-app-intro-slider": "^3.0.0",
    "react-native-camera": "^3.3.2",
    "react-native-config": "^0.11.7",
    "react-native-device-info": "^5.3.1",
    "react-native-gesture-handler": "^1.4.1",
    "react-native-permissions": "^1.2.0",
    "react-native-qrcode-scanner": "^1.2.1",
    "react-native-reanimated": "^1.2.0",
    "react-native-rsa-native": "^1.1.2",
    "react-native-screens": "1.0.0-alpha.23",
    "react-native-sha256": "^1.3.6",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^9.9.3",
    "react-native-vector-icons": "^6.6.0",
    "react-redux": "^7.1.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.1.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.5.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "eslint": "^6.2.2",
    "eslint-plugin-react-hooks": "^2.0.1",
    "import-sort-cli": "^6.0.0",
    "jest": "^24.9.0",
    "jetifier": "^1.6.4",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-native-cli": "^2.0.1",
    "react-native-log-ios": "^1.0.1",
    "react-native-svg-transformer": "^0.14.2",
    "react-test-renderer": "16.8.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
