{
  "name": "EasyAccess",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "^10.16.3",
    "yarn": "^1.17.3"
  },
  "scripts": {
    "android": "ENVFILE=.env.development android/gradlew -p android installDevelopmentDebug && adb reverse tcp:8081 tcp:8081 && adb shell am start -n org.itsrio.sefaz.development.easyaccess/com.easyaccess.MainActivity",
    "build-android-staging": "ENVFILE=.env.staging android/gradlew -p android assembleStagingRelease",
    "ios": "ENVFILE=.env.development react-native run-ios --scheme EasyAccess",
    "lint": "eslint .",
    "log-android": "react-native log-android",
    "log-ios": "react-native-log-ios EasyAccess",
    "prettier": "prettier --write index.js src/*.js src/**/*.js",
    "start": "react-native start",
    "sort": "import-sort --write index.js src/*.js src/**/*.js",
    "test": "jest",
    "xcode": "xed ios"
  },
  "dependencies": {
    "react": "16.8.6",
    "react-native": "0.60.5",
    "react-native-config": "^0.11.7",
    "react-redux": "^7.1.1",
    "recompose": "^0.30.0",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.1.0",
    "rxjs": "^6.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.9.0",
    "eslint": "^6.2.2",
    "import-sort-cli": "^6.0.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "react-native-cli": "^2.0.1",
    "react-native-log-ios": "^1.0.1",
    "react-test-renderer": "16.8.6"
  },
  "jest": {
    "preset": "react-native"
  }
}
